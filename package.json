{
  "name": "@livenovel/framework",
  "version": "1.0.0",
  "description": "LiveNovel controller framework",
  "main": "dist/live2dcubismframework.js",
  "exports": {
    ".": "./dist/live2dcubismframework.js",
    "./*": "./dist/*.js"
  },
  "types": "dist/live2dcubismframework.d.ts",
  "typesVersions": {
    "*": {
      "dist/live2dcubismframework.d.ts": [
        "./dist/live2dcubismframework.d.ts"
      ],
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "typescript": "4.7.2"
  }
}
